[
  {
    "$h1": {
      "class": "bheader",
      "text": "Polip"
    }
  },
  {
    "$div": {
      "children": [
        {
          "$div": {
            "children": [
              {
                "$span": {
                  "class": "gtitle",
                  "text": "Adet"
                }
              },
              {
                "$input": {
                  "type": "number",
                  "id": "adet",
                  "style": "width:100px;",
                  "placeholder": "Adet",
                  "(onkeyup)": "formchanger"
                }
              }
            ],
            "class": "fgroup"
          }
        },
        {
          "$div": {
            "children": [
              {
                "$span": {
                  "class": "gtitle",
                  "text": "Tipi"
                }
              },
              {
                "$div": {
                  "children": [
                    {
                      "$label": {
                        "children": [
                          {
                            "$input": {
                              "type": "checkbox",
                              "id": "sapli",
                              "value": "Sapl覺",
                              "(onclick)": "formchanger"
                            },
                            "$label":{
                                "for":"sapli",
                                "text":"Sapl覺"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "$label": {
                        "children": [
                          {
                            "$input": {
                              "type": "checkbox",
                              "id": "sapsiz",
                              "value": "Saps覺z",
                              "(onclick)": "formchanger"
                            },
                            "$label":{
                                "for":"sapsiz",
                                "text":"Saps覺z"
                            }
                          }
                        ]
                      }
                    }
                  ],
                  "class": "group-label-list"
                }
              }
            ],
            "class": "fgroup"
          }
        },
        {
          "$div": {
            "children": [
              {
                "$span": {
                  "class": "gtitle",
                  "text": "Boyut"
                }
              },
              {
                "$div": {
                  "children": [
                    {
                      "$input": {
                        "type": "checkbox",
                        "id": "boyut1",
                        "value": "<0.5 cm",
                        "(onclick)": "formchanger"
                      }
                    },
                    {
                      "$label": {
                        "for": "boyut1",
                        "text": "<0.5 cm"
                      }
                    },
                    {
                      "$input": {
                        "type": "checkbox",
                        "id": "boyut2",
                        "value": "0.5-1 cm",
                        "(onclick)": "formchanger"
                      }
                    },
                    {
                      "$label": {
                        "for": "boyut2",
                        "text": "0.5-1 cm"
                      }
                    },
                    {
                      "$input": {
                        "type": "checkbox",
                        "id": "boyut3",
                        "value": "1-2 cm",
                        "(onclick)": "formchanger"
                      }
                    },
                    {
                      "$label": {
                        "for": "boyut3",
                        "text": "1-2 cm"
                      }
                    },
                    {
                      "$input": {
                        "type": "checkbox",
                        "id": "boyut4",
                        "value": ">3 cm",
                        "(onclick)": "formchanger"
                      }
                    },
                    {
                      "$label": {
                        "for": "boyut4",
                        "text": ">3 cm"
                      }
                    }
                  ],
                  "class": "group-label-list"
                }
              }
            ],
            "class": "fgroup"
          }
        }
      ],
      "id": "modalpage-content"
    }
  },
  {
    "$div": {
      "children": [
        {
          "$input": {
            "type": "button",
            "value": "Kaydet",
            "(onclick)": "close"
          }
        }
      ]
    }
  }
]